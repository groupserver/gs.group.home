<div id="us-bar" class="ui-widget-content ui-corner-all">
  <h2>Us</h2>  
  <ul id="us-photos" class="inline-list">
    <tal:block repeat="member view/postingMembers">
      <li>
        <a tal:attributes="title string:Posts by ${member/name};
          href string:/s?t=0&amp;p=1&amp;a=${member/id}&amp;g=${view/groupInfo/id}">
          <div tal:define="user member/user"
            tal:replace="structure provider:groupserver.UserImage"/>
        </a>
      </li>
    </tal:block>
  </ul>
  <div id="us-tasks" 
    tal:content="structure provider:groupserver.GroupHomepageUsBarLinks">
    &#160;
  </div>
</div><!--us-bar-->

